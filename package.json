        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.2",
        "@vue/tsconfig": "^0.4.0",
        "eslint": "^8.56.0",
        "eslint-plugin-vue": "^9.20.1",
        "jsdom": "^23.0.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.33",
        "postcss-prefix-selector": "^1.16.0",
        "sass": "^1.70.0",
        "typescript": "^5.3.3",
        "vite": "^5.0.12",
        "vitepress": "^1.0.0-rc.40",
        "vitest": "^1.2.1",
        "vue-tsc": "^1.8.27"
