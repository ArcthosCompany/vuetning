{
  "name": "vuetning",
  "version": "0.0.0",
  "private": false,
  "description": "Salesforce Lightning Design System framework for Vue.js 2",
  "author": "Marcelo A. T. Gomes <marceloatg@outlook.com> (https://github.com/marceloatg)",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --no-fix",
    "bundle": "vue-cli-service build --target lib --name vuetning src/index.js",
    "lint:fix": "vue-cli-service lint",
    "lint:teamcity": "vue-cli-service lint --no-fix --format ./node_modules/eslint-teamcity/index.js"
  },
  "main": "./dist/vuetning.common.js",
  "unpkg": "dist/vuetning.umd.min.js",
  "files": [
    "dist/*.js",
    "src/components/*",
    "src/directives/*",
    "src/mixins/*",
    "src/utils/*",
    "src/index.js"
  ],
  "dependencies": {
    "core-js": "^3.22.4",
    "duration": "^0.2.2",
    "highlight.js": "^10.6.0",
    "humanize-duration": "^3.27.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "numeral": "^2.0.6",
    "quill": "^1.3.7",
    "scrollparent": "^2.0.1",
    "tippy.js": "^6.3.7",
    "uuid": "^8.3.2",
    "v-clipboard": "^2.2.3",
    "vue": "^2.6.14",
    "vue-observe-visibility": "^1.0.0",
    "vue-router": "^3.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-router": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-teamcity": "^4.0.0",
    "jest-teamcity-reporter": "^0.9.0",
    "sass": "^1.51.0",
    "sass-loader": "^10.2.0",
    "viewcode": "0.0.27",
    "vue-template-compiler": "^2.6.14"
  },
  "bugs": {
    "url": "https://github.com/marceloatg/vuetning/issues"
  },
  "contributors": [
    "Marcelo A. T. Gomes <marceloatg@outlook.com> (https://github.com/marceloatg)",
    "Vinicius Damiati <viniciusdamiati@gmail.com> (https://github.com/ViniciusDamiati)"
  ],
  "homepage": "https://marceloatg.github.io/vuetning/",
  "jsdelivr": "dist/vuetning.umd.min.js",
  "keywords": [
    "vuetning",
    "vue",
    "vuejs",
    "slds",
    "lightning",
    "salesforce",
    "framework"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marceloatg/vuetning"
  }
}
