        "numeral": "^2.0.6",
        "scrollparent": "^2.1.0",
        "uuid": "^9.0.1",
        "vue": "^3.4.15",
        "vue-observe-visibility": "^2.0.0-alpha.1"
    },
    "devDependencies": {
        "@babel/types": "^7.23.4",
        "@rushstack/eslint-patch": "^1.6.0",
