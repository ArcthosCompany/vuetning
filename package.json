{
  "name": "vuetning",
  "version": "0.0.0",
  "private": false,
  "description": "Salesforce Lightning Design System framework for Vue.js 2",
  "author": "Marcelo A. T. Gomes <marceloatg@outlook.com> (https://github.com/marceloatg)",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --collectCoverage",
    "test:teamcity": "jest",
    "lint": "vue-cli-service lint --no-fix",
    "bundle": "vue-cli-service build --target lib --name vuetning src/index.js",
    "lint:fix": "vue-cli-service lint",
    "lint:teamcity": "vue-cli-service lint --no-fix --format ./node_modules/eslint-teamcity/index.js"
  },
  "main": "./dist/vuetning.common.js",
  "unpkg": "dist/vuetning.umd.min.js",
  "files": [
    "dist/*.js",
    "src/components/*",
    "src/directives/*",
    "src/mixins/*",
    "src/utils/*",
    "src/index.js"
  ],
  "dependencies": {
    "core-js": "^3.8.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "numeral": "^2.0.6",
    "quill": "^1.3.7",
    "scrollparent": "^2.0.1",
    "uuid": "^8.3.2",
    "v-clipboard": "^2.2.3",
    "vue": "^2.7.0",
    "vue-observe-visibility": "^1.0.0",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@types/jest": "^27.0.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-teamcity": "^4.0.0",
    "jest": "^27.0.5",
    "jest-teamcity": "^1.11.0",
    "jest-teamcity-reporter": "^0.9.0",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0"
  },
  "bugs": {
    "url": "https://github.com/marceloatg/vuetning/issues"
  },
  "contributors": [
    "Marcelo A. T. Gomes <marceloatg@outlook.com> (https://github.com/marceloatg)",
    "Vinicius Damiati <viniciusdamiati@gmail.com> (https://github.com/ViniciusDamiati)"
  ],
  "homepage": "https://marceloatg.github.io/vuetning/",
  "jsdelivr": "dist/vuetning.umd.min.js",
  "keywords": [
    "vuetning",
    "vue",
    "vuejs",
    "slds",
    "lightning",
    "salesforce",
    "framework"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marceloatg/vuetning"
  }
}
